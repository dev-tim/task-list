{
  "name": "wunder-challenge",
  "version": "1.0.0",
  "description": "Wunder challenge to build todolist",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "compile": "node -r babel-register ./node_modules/.bin/webpack -p --config=webpack.config.js",
    "start": "node --harmony server.js",
    "dev": "concurrently \"node --harmony webpack-dev-server.js\" \"node --harmony server.js\"",
    "test": "node ./node_modules/karma/bin/karma start ./karma.conf.js --harmony",
    "wundertree": "node -r babel-register ./scripts/wundertree",
    "test:wundertree": "./node_modules/.bin/mocha  -r ./babel-init-server.js -r chai  scripts/**/*.spec.js",
    "deploy": "npm run test && npm run compile",
    "postinstall": "npm run compile"
  },
